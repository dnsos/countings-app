<% if user_signed_in? %>
  <%= link_to "#{current_user.email}", profile_path %>
  <%= button_to "Logout", destroy_user_session_path, method: :delete, data: { turbo: false } %>
<% else %>
  <%= link_to "Login", new_user_session_path %>
  <%= link_to "Register", new_user_registration_path %>
<% end %>